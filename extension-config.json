{
  "version": "1.0.0",
  "id": "@shopgate/bigcommerce-products",
  "directories": {
    "steps": "./extension/",
    "pipelines": "./pipelines/"
  },
  "steps": [],
  "configuration": {
    "cakeUrl": {
      "type": "static",
      "destination": "backend",
      "params": {
        "value": "https://rapid-php.shopgate.com/"
      }
    },
    "languageId": {
      "type": "bigApi",
      "destination" : "backend",
      "params": {
        "method" : "GET",
        "service" : "shop",
        "path" : "/v1/%(shopId)s",
        "key" : "languageId"
      }
    },
    "cloudsearch-de": {
      "type": "static",
      "destination": "backend",
      "params": {
        "value": "https://search-shopgate-items-de-hdxp45pgwwdvd4umvpq36rejwe.eu-west-1.cloudsearch.amazonaws.com/2013-01-01/search"
      }
    },
    "cloudsearch-en": {
      "type": "static",
      "destination": "backend",
      "params": {
        "value": "https://search-shopgate-items-en-qj4xjcc45jah5zdcxwoqbriivi.eu-west-1.cloudsearch.amazonaws.com/2013-01-01/search"
      }
    },
    "requestTimeout": {
      "type": "static",
      "destination": "backend",
      "params": {
        "value": "15000"
      }
    }
  }
}
